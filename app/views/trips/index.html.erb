<%= render "shared/container" do %>
  <h1>Trips</h1>
  <ul>
    <% @trips.each do |trip| %>
      <li><%= link_to trip.name, trip_path(trip) %></li>
    <% end %>
    <li>
      <%= link_to "Create trip", new_trip_path %>
      <%= link_to "View Preferences", potential_destinations_path %>
    </li>
  </ul>
<% end %>
